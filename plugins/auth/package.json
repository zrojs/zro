{
  "name": "@zro/auth",
  "version": "0.0.4",
  "type": "module",
  "description": "Auth integration for ZRO",
  "scripts": {
    "dev": "unbuild --watch",
    "build": "unbuild & tsup --dts-only",
    "prepublishOnly": "pnpm build && pnpx changelogen --release --no-tag --push"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./providers/*": {
      "types": "./dist/providers/*/index.d.ts",
      "default": "./dist/providers/*/index.mjs"
    }
  },
  "keywords": [],
  "author": "Nariman Movaffaghi",
  "license": "MIT",
  "packageManager": "pnpm@10.6.3",
  "devDependencies": {
    "tsup": "^8.4.0",
    "unbuild": "^3.5.0"
  },
  "peerDependencies": {
    "zro": "workspace:^*"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "tinyglobby": "^0.2.12",
    "zod": "^3.24.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
