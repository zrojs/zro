{
  "name": "@zro/db",
  "version": "0.0.3",
  "type": "module",
  "description": "unjs/db0 integration for ZRO apps",
  "scripts": {
    "dev": "unbuild --watch",
    "build": "unbuild",
    "prepublishOnly": "pnpm build && pnpx changelogen --release --no-tag --push"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./connectors/libsql/*": {
      "types": "./dist/connectors/libsql/*.d.ts",
      "default": "./dist/connectors/libsql/*.mjs"
    },
    "./connectors/*": {
      "types": "./dist/connectors/*.d.ts",
      "default": "./dist/connectors/*.mjs"
    }
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Nariman Movaffaghi",
  "license": "MIT",
  "packageManager": "pnpm@10.6.3",
  "dependencies": {
    "db0": "^0.3.1",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "unbuild": "^3.5.0"
  },
  "peerDependencies": {
    "drizzle-orm": ">=0.41",
    "zro": "workspace:^*"
  },
  "peerDependenciesMeta": {
    "drizzle-orm": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
